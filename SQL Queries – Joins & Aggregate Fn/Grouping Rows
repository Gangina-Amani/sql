GROUP BY:
create view calculations as select deptno, min(sal) as "MIN SAL", max(sal) as "MAX SAL", avg(sal) as "AVG SAL" from emp group by deptno;

ORDER BY:
SELECT rollno,sname, CONCAT('19K61A0',rollno) regid FROM student order by rollno;

To fetch first row from table:
select eno, ename, esal, edept from emp3 order by esal desc fetch first row only;
