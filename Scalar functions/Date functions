SELECT NOW();

SELECT CURDATE();

SELECT Name, DATE(BirthTime) AS BirthDate FROM Test;

SELECT Name, Extract(YEAR FROM BirthTime) AS BirthYear FROM Test;

SELECT DATE('2002-01-23 05:06:07');

SELECT FROM_DAYS(766689);

SELECT HOUR('12:00:00');

SELECT LAST_DAY('2002-01-23');

SELECT MAKETIME(11,14,30);
